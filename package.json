{
  "name": "vite-template-sky",
  "version": "0.0.0",
  "description": "an opinionated vite starter template for vue or react with a cli to generate template project",
  "keywords": [
    "template",
    "project",
    "vite",
    "cli"
  ],
  "bin": {
    "create-vite-template-sky": "index.js"
  },
  "files": [
    "index.js",
    "dist",
    "vite-react-template-sky",
    "vite-vue-template-sky"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skyclouds2001/vite-template-sky.git"
  },
  "homepage": "https://github.com/skyclouds2001/vite-template-sky#readme",
  "bugs": {
    "url": "https://github.com/skyclouds2001/vite-template-sky/issues",
    "email": "skyclouds2001@163.com"
  },
  "license": "MIT",
  "author": {
    "name": "skyclouds2001",
    "email": "skyclouds2001@163.com",
    "url": "https://skyclouds2001.github.io/"
  },
  "contributors": [
    "skyclouds2001"
  ],
  "engines": {
    "node": ">=18.13.0",
    "npm": ">=9.6.0",
    "yarn": ">=1.22.0",
    "pnpm": ">=8.4.0"
  },
  "scripts": {
    "dev": "rollup --config --bundleConfigAsCjs --watch",
    "build": "rollup --config --bundleConfigAsCjs",
    "lint": "eslint --fix . && prettier --write . && markdownlint --fix **/*.md && tsc --noEmit",
    "test": "vitest",
    "clean": "rimraf node_modules && rimraf dist",
    "changeset": "changeset",
    "bump": "changeset version",
    "commit": "cz",
    "prepare": "is-ci || husky install",
    "preinstall": "only-allow pnpm"
  },
  "dependencies": {
    "kleur": "^4.1.5",
    "minimist": "^1.2.8",
    "prompts": "^2.4.2",
    "simple-git": "^3.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.10.4",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vitest": "^0.3.17",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.0",
    "markdownlint": "^0.32.1",
    "markdownlint-cli": "^0.38.0",
    "only-allow": "^1.2.1",
    "pnpm": "8.10.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.5",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  }
}
